<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:com:nh:db:ml:simuservice:model">	
	<xs:element name="SlaInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="clients" type="xs:string"
					maxOccurs="unbounded" minOccurs="0"/>
				<xs:element name="cdns" type="xs:string"
					maxOccurs="unbounded" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="sladelay" type="xs:int"/>
			<xs:attribute name="vcdnratio" type="xs:int"/>
			<xs:attribute name="bandwidth" type="xs:int"/>
			<xs:attribute name="nbuser" type="xs:int"/>
		</xs:complexType>
	</xs:element>
</xs:schema>